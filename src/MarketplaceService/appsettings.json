{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MarketplaceSettings": {
    "TradingSettings": {
      "MinOrderValue": 10.00,
      "MaxOrderValue": 1000000.00,
      "DefaultTradingFee": 0.001,
      "VipTradingFee": 0.0005,
      "MaxSlippage": 0.05,
      "OrderTimeoutMinutes": 30
    },
    "AssetManagement": {
      "SupportedAssets": ["BTC", "ETH", "USDT", "USDC", "BNB"],
      "MinimumLiquidity": 100000.00,
      "PriceUpdateIntervalSeconds": 5,
      "VolatilityThreshold": 0.10,
      "CircuitBreakerEnabled": true
    },
    "OrderMatching": {
      "MatchingAlgorithm": "PriceTimePriority",
      "BatchProcessingEnabled": true,
      "BatchSizeLimit": 1000,
      "MatchingIntervalMs": 100,
      "PartialFillsAllowed": true
    },
    "MarketMaking": {
      "EnableMarketMaking": true,
      "SpreadTargetBps": 5,
      "InventoryLimits": {
        "BTC": 10.0,
        "ETH": 100.0,
        "USDT": 1000000.0
      },
      "QuoteRefreshIntervalSeconds": 1
    }
  },
  "LiquiditySettings": {
    "LiquidityProviders": {
      "MinimumStake": 50000.00,
      "RewardDistributionFrequency": "Daily",
      "LiquidityMiningEnabled": true,
      "ImpermanentLossProtection": true
    },
    "PoolManagement": {
      "AutoRebalancing": true,
      "RebalanceThreshold": 0.02,
      "MaxPoolSize": 10000000.00,
      "MinPoolSize": 100000.00,
      "PoolCreationFee": 1000.00
    },
    "YieldFarming": {
      "EnableYieldFarming": true,
      "BaseAPY": 0.05,
      "BonusMultiplier": 1.5,
      "LockupPeriods": [30, 90, 180, 365],
      "EarlyWithdrawalPenalty": 0.02
    }
  },
  "SecuritySettings": {
    "TradingLimits": {
      "DailyTradingLimit": 1000000.00,
      "HourlyTradingLimit": 100000.00,
      "SuspiciousActivityThreshold": 50000.00,
      "VelocityCheckEnabled": true
    },
    "RiskManagement": {
      "MaxPositionSize": 500000.00,
      "MarginRequirement": 0.10,
      "LiquidationThreshold": 0.80,
      "RiskScoreThreshold": 85,
      "AutoLiquidationEnabled": true
    },
    "Compliance": {
      "KycRequired": true,
      "AmlScreening": true,
      "GeoblockingEnabled": true,
      "RestrictedCountries": ["US", "CN", "KP"],
      "TransactionReporting": true
    }
  },
  "ExternalIntegrations": {
    "PriceOracles": {
      "PrimaryOracle": "Chainlink",
      "SecondaryOracle": "Band Protocol",
      "FallbackOracle": "CoinGecko",
      "PriceDeviationThreshold": 0.01,
      "OracleTimeoutSeconds": 30
    },
    "PaymentGateways": {
      "CryptocurrencyGateways": ["Binance", "Coinbase", "Kraken"],
      "FiatGateways": ["Stripe", "PayPal", "Wire"],
      "SettlementTimeMinutes": 15,
      "PaymentTimeoutMinutes": 30
    },
    "Exchanges": {
      "ConnectedExchanges": ["Binance", "Coinbase", "Uniswap"],
      "ArbitrageEnabled": true,
      "CrossExchangeTrading": true,
      "LatencyThresholdMs": 100
    }
  },
  "UserExperience": {
    "Interface": {
      "DefaultTheme": "Dark",
      "AdvancedTradingView": true,
      "MobileOptimized": true,
      "MultiLanguageSupport": true,
      "SupportedLanguages": ["en", "es", "fr", "de", "ja", "ko"]
    },
    "Notifications": {
      "PriceAlerts": true,
      "OrderFillNotifications": true,
      "MarketNewsEnabled": true,
      "EmailNotifications": true,
      "PushNotifications": true
    },
    "Analytics": {
      "TradingAnalytics": true,
      "PortfolioTracking": true,
      "PnLReporting": true,
      "TaxReporting": true,
      "PerformanceBenchmarking": true
    }
  },
  "PerformanceOptimization": {
    "Caching": {
      "PriceCacheTTLSeconds": 5,
      "OrderBookCacheTTLSeconds": 1,
      "UserDataCacheTTLMinutes": 15,
      "StaticDataCacheTTLHours": 24
    },
    "LoadBalancing": {
      "EnableLoadBalancing": true,
      "MaxConcurrentUsers": 10000,
      "RequestRateLimitPerSecond": 100,
      "BurstLimit": 500
    },
    "DatabaseOptimization": {
      "ConnectionPoolSize": 50,
      "QueryTimeoutSeconds": 30,
      "IndexOptimization": true,
      "PartitioningEnabled": true
    }
  },
  "Monitoring": {
    "Metrics": {
      "TradingVolumeTracking": true,
      "LatencyMonitoring": true,
      "ErrorRateMonitoring": true,
      "UserActivityTracking": true
    },
    "Alerts": {
      "HighLatencyAlert": 500,
      "ErrorRateAlert": 0.01,
      "VolumeAnomalyAlert": true,
      "SystemHealthAlert": true
    }
  }
}
